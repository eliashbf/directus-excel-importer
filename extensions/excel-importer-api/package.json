{
	"name": "excel-importer-api",
	"description": "Extension para importar datos desde archivos Excel en Directus",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.2",
		"@types/node": "^24.10.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@types/busboy": "^1.5.4",
		"busboy": "^1.6.0",
		"xlsx": "^0.18.5"
	}
}
